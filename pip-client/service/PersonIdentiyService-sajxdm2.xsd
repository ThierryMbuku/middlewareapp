   </xsd:complexType>
   <xsd:complexType name="ImageType">
      <xsd:annotation>
         <xsd:documentation source="model">A structure that describes a representation of an image.</xsd:documentation>
      </xsd:annotation>
      <xsd:complexContent mixed="false">
         <xsd:extension base="saj:BinaryType"/>
      </xsd:complexContent>
   </xsd:complexType>
   <xsd:complexType name="MaritalStatusCodeType">
      <xsd:annotation>
         <xsd:documentation source="model">ICS codeset type created for element 'PersonMaritalStatusCode'.</xsd:documentation>
      </xsd:annotation>
      <xsd:simpleContent>
         <xsd:restriction base="saj:ReferenceDataType">
            <xsd:attribute name="Type" type="saj:ReferenceDataTypeType" use="required">
               <xsd:annotation>
                  <xsd:documentation source="model">IJS ICS reference data set type. Only 'ICS_refdata' is valid.</xsd:documentation>
               </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute fixed="MaritalStatus" name="Name" type="saj:ReferenceDataNameType" use="required">
               <xsd:annotation>
                  <xsd:documentation source="model">IJS ICS reference data set name as it appears in the ICS system.</xsd:documentation>
               </xsd:annotation>
            </xsd:attribute>
            <xsd:attribute name="Version" type="saj:ReferenceDataVersionType" use="required">
