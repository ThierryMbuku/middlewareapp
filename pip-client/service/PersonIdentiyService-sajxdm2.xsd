                  <xsd:documentation source="model">IJS ICS reference data set version as it appears in the ICS system.</xsd:documentation>
               </xsd:annotation>
            </xsd:attribute>
         </xsd:restriction>
      </xsd:simpleContent>
   </xsd:complexType>
   <xsd:complexType name="DHA_PersonIdentityProfileType">
